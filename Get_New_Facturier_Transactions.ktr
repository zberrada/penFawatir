    <sql>select

nom_en_gb as pays,

City.sname as ville ,

product.sname as fournisseur,

month (dInsertion) as mois,

count(*) as nbr,

sum(namount) as montant

from transactions 

inner join Shop on nShopId = Shop.nId and len(sshopid) = 4

inner join  view_partners on view_partners.nTransactionId = transactions.nid

left join city on Shop.ncityid = city.nid

left join product on product.nid = transactions.nproductid

left join country on (country=iso2 or country=iso3)

where transactions.dInsertion >= '2020-01-01'

and transactions.dInsertion &lt; '2020-04-01'

and transactions.nProductId not in (1,19)

and nTransactionStatusId in (8,26) 

group by  nom_en_gb,City.sname,product.sname, month (dInsertion)

​</sql>
